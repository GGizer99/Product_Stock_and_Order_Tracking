@using Product_Stock_and_Order_Tracking.Models
@model List<Users>
@{
    ViewBag.Title = "UserList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-hover">
    <tr style="background-color:silver">

        <th>Bayi Adı</th>
        <th>Bayi Sahibi Adı</th>
        <th>Bayi Sahibi Soyadı</th>
        <th>Bayi Sahibi Email</th>
        <th>Bayi Telefon Numarası</th>
        <th>Bayi Adresi</th>
        
    </tr>

    @foreach (var user in Model)
    {
        <tr>

            <td>@user.BayiAdi</td>
            <td>@user.Name</td>
            <td>@user.Surname</td>
            <td>@user.Email</td>
            <td>@user.BayiTel</td>
            <td>@user.BayiAdres</td>

        </tr>
    }
</table>

